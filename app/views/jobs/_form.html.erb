<%= simple_form_for @job do |f| %>
  <div class="form-group">

    <%= f.input :title, input_html: { class: "form-control"} %>
    <%= f.input :description, input_html: { class: "form-control"} %>
    <%= f.input :email, input_html: { class: "form-control"} %>
  <%= f.input :minsalary, input_html: { class: "form-control"} %>
  <%= f.input :maxsalary, input_html: { class: "form-control"} %>




</div>
  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submitting..." } %>
<% end %>
