<%= form_for @job do |f| %>

  职缺名称
  <%= f.text_field :title %>
  <br>
  职缺描述
  <br>
  <%= f.text_area :descripiton %>
  <br>
  工资下限
  <br>
  <%= f.number_field :wage_lower_bound %>
  <br>
  <br>
  工资上限
  <br>
  <%= f.number_field :wage_upper_bound %>
  <br>
  联系邮箱
  <br>
  <%= f.email_field :contact_email %>
  <br>
  <%= f.submit "提交职缺", :disable_with => '提交中。。。' %>

<% end %>
